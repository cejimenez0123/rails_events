<h1>Evento</h1>
    <h3>Welcome, <%= @user.name.capitalize %></h3>
    <h4>Events:</h4>

    <%if !@user.created_events.empty? %>     
        <% @user.created_events.each do |event|%>
        <ul>
    <li><%= link_to "#{event.name}", edit_event_path(event)%></li>
        <br>
        </ul>
    <%end%>
    <%else%>
        <p>Create some events down below</p>
    <%end%>
    <%= link_to "Events", events_path%>
    <%= link_to "Your RSVPs", rsvps_path%>
    <%= link_to "Create Event", new_event_path%>