<h1>Evento</h1>
    <h3>Welcome, <%= @user.name.capitalize %></h3>
    <h4>Events:</h4>

    <%if !@user.created_events.empty? %>     
        <% @user.created_events.each do |event|%>
        <ul>
    <li><%= link_to "#{event.name}", edit_user_event_path(@user,event)%></li>
        <br>
        </ul>
    <%end%>
    <%else%>
        <p>Create some events down below</p>
    <%end%>
    <%= render partial: "statics/links"%>